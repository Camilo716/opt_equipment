<data>
    <xpath expr="//field[@name='number']" position="before">
	<field name="quote_number"/>
    </xpath>
    <xpath expr="//field[@name='number']" position="replace">
	<field name="number"/>
    </xpath>
    <xpath expr="//field[@name='untaxed_amount']" position="after">
	<field name="residual_amount"/>
    </xpath>
</data>
